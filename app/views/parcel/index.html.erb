<h1>INDEX</h1>
<table class="table table-striped table-hover">
    <thead class="">
        <tr>
            <th>ID</th>
            <th>Volume</th>
            <th>Weight</th>
            <th>Price</th>
            <th>Created_at</th>
            <th>Distance</th>
            <th>From</th>
            <th>To</th>
        </tr>
    </thead>
    <tbody>
        <% @cities.each do |city| %>
        <tr>
            <td><%= city["id"] %></td>
            <td><%= city["volume"] %></td>
            <td><%= city["weight"] %></td>
            <td><%= city["price"] %></td>
            <td><%= city["created_at"] %></td>
            <td><%= city["distance"] %></td>
            <td><%= city["from_name"] %></td>
            <td><%= city["to_name"] %></td>
        <% end %>
        </tr>
    </tbody>
</table>